{"ast":null,"code":"import { productsService } from \"@/public/services/productsService\";\nexport default {\n  props: {\n    isEditMode: {\n      type: Boolean,\n      default: false\n    },\n    restaurantName: {\n      type: String,\n      required: true\n    },\n    cart: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      searchQuery: '',\n      products: [],\n      // Almacenar todos los productos del restaurante\n      filteredProducts: [] // Productos filtrados por la búsqueda\n    };\n  },\n  mounted() {\n    if (this.restaurantName) {\n      this.loadProducts();\n    } else {\n      console.error('Restaurant name is required');\n    }\n  },\n  methods: {\n    async loadProducts() {\n      console.log('Restaurant Name: ', this.restaurantName);\n      try {\n        const products = await productsService.getProductsByRestaurant(this.restaurantName);\n        this.products = products;\n        this.filteredProducts = this.products;\n      } catch (error) {\n        console.log(\"Failed to load products\", error);\n      }\n    },\n    filterProducts() {\n      const query = this.searchQuery.toLowerCase();\n      this.filteredProducts = this.products.filter(product => product.name.toLowerCase().includes(query));\n    },\n    addProductToCart(product) {\n      this.$emit('add-to-cart', product);\n    },\n    toggleEditMode() {\n      this.$emit('toggle-edit-mode');\n    }\n  }\n};","map":{"version":3,"names":["productsService","props","isEditMode","type","Boolean","default","restaurantName","String","required","cart","Array","data","searchQuery","products","filteredProducts","mounted","loadProducts","console","error","methods","log","getProductsByRestaurant","filterProducts","query","toLowerCase","filter","product","name","includes","addProductToCart","$emit","toggleEditMode"],"sources":["E:\\kitchen-tech-repo\\src\\admins\\views\\cassing-views\\components\\favorite-product-header.vue"],"sourcesContent":["<template>\r\n  <div class=\"products-header\">\r\n    <input\r\n        class=\"search-bar\"\r\n        type=\"text\"\r\n        v-model=\"searchQuery\"\r\n        placeholder=\"Search products...\"\r\n        @input=\"filterProducts\"\r\n    />\r\n    <ul v-if=\"filteredProducts.length && searchQuery\" class=\"dropdown\">\r\n      <li v-for=\"product in filteredProducts\" :key=\"product.id\" class=\"product-card\">\r\n        <div class=\"card-content\">\r\n          <span>{{ product.name }} - S/.{{ product.price }}</span>\r\n          <button @click=\"addProductToCart(product)\" class=\"add-button\">Add</button>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <button class=\"edit-button\" @click=\"toggleEditMode\">Edit</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { productsService } from \"@/public/services/productsService\";\r\n\r\nexport default {\r\n  props: {\r\n    isEditMode: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n    restaurantName: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n    cart: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      products: [], // Almacenar todos los productos del restaurante\r\n      filteredProducts: [], // Productos filtrados por la búsqueda\r\n    };\r\n  },\r\n  mounted() {\r\n    if(this.restaurantName) {\r\n      this.loadProducts();\r\n    } else {\r\n      console.error('Restaurant name is required');\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    async loadProducts() {\r\n      console.log('Restaurant Name: ', this.restaurantName);\r\n      try {\r\n        const products = await productsService.getProductsByRestaurant(this.restaurantName);\r\n        this.products = products;\r\n        this.filteredProducts = this.products;\r\n      } catch (error) {\r\n        console.log(\"Failed to load products\", error);\r\n      }\r\n    },\r\n    filterProducts() {\r\n      const query = this.searchQuery.toLowerCase();\r\n      this.filteredProducts = this.products.filter((product) => product.name.toLowerCase().includes(query));\r\n    },\r\n    addProductToCart(product) {\r\n      this.$emit('add-to-cart', product);\r\n    },\r\n    toggleEditMode() {\r\n      this.$emit('toggle-edit-mode');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.dropdown li {\r\n  padding: 10px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n.dropdown button {\r\n  background-color: #31304A;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n.products-header {\r\n   display: flex;\r\n   max-width: 1000px;\r\n   justify-content: space-between;\r\n   align-items: center;\r\n   margin: 0 auto;\r\n   padding-bottom: 10px;\r\n   gap: 10px;\r\n }\r\n.search-bar {\r\n  width: 100%;\r\n  padding: 15px;\r\n  border-radius: 5px;\r\n  border: none;\r\n  color: #31304A;\r\n  background-color: #D3D2E5;\r\n}\r\n.edit-button {\r\n  padding: 13px 20px;\r\n  background: #D3D2E5;\r\n  color: #31304A;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-weight: 800;\r\n  flex-shrink: 0;\r\n}\r\n</style>"],"mappings":"AAsBA,SAASA,eAAc,QAAS,mCAAmC;AAEnE,eAAe;EACbC,KAAK,EAAE;IACLC,UAAU,EAAE;MACVC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX,CAAC;IACDC,cAAc,EAAE;MACdH,IAAI,EAAEI,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,IAAI,EAAE;MACJN,IAAI,EAAEO,KAAK;MACXF,QAAQ,EAAE;IACZ;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MAAE;MACdC,gBAAgB,EAAE,EAAE,CAAE;IACxB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAG,IAAI,CAACT,cAAc,EAAE;MACtB,IAAI,CAACU,YAAY,CAAC,CAAC;IACrB,OAAO;MACLC,OAAO,CAACC,KAAK,CAAC,6BAA6B,CAAC;IAC9C;EAEF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMH,YAAYA,CAAA,EAAG;MACnBC,OAAO,CAACG,GAAG,CAAC,mBAAmB,EAAE,IAAI,CAACd,cAAc,CAAC;MACrD,IAAI;QACF,MAAMO,QAAO,GAAI,MAAMb,eAAe,CAACqB,uBAAuB,CAAC,IAAI,CAACf,cAAc,CAAC;QACnF,IAAI,CAACO,QAAO,GAAIA,QAAQ;QACxB,IAAI,CAACC,gBAAe,GAAI,IAAI,CAACD,QAAQ;MACvC,EAAE,OAAOK,