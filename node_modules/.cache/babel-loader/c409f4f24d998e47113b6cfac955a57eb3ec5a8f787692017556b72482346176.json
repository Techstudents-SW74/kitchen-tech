ct);\n    },\n    toggleEditMode() {\n      this.$emit('toggle-edit-mode');\n    },\n    handleClickOutside(event) {\n      if (!this.$el.contains(event.target)) {\n        this.filteredProducts = [];\n      }\n    },\n    onSearchFocus() {\n      if (this.searchQuery) {\n        this.filterProducts();\n      }\n    }\n  }\n};","map":{"version":3,"names":["productsService","props","isEditMode","type","Boolean","default","restaurantName","String","required","cart","Array","selectedSlot","Number","data","searchQuery","products","filteredProducts","mounted","loadProducts","console","error","document","addEventListener","handleClickOutside","beforeUnmount","removeEventListener","methods","userData","JSON","parse","localStorage","getItem","restaurantId","getProductsByRestaurant","filterProducts","query","toLowerCase","filter","product","productName","includes","log","addProductToSlot","$emit","slot","addProductToCart","toggleEditMode","event","$el","contains","target","onSearchFocus"],"sources":["E:\\kitchen-tech-repo\\src\\admins\\views\\cassing-views\\components\\favorite-product-header.vue"],"sourcesContent":["<template>\r\n  <div class=\"products-header\">\r\n    <input\r\n        class=\"search-bar\"\r\n        type=\"text\"\r\n        v-model=\"searchQuery\"\r\n        placeholder=\"Search products...\"\r\n        @input=\"filterProducts\"\r\n        @focus=\"onSearchFocus\"\r\n    />\r\n    <ul v-if=\"filteredProducts.length && searchQuery\" class=\"dropdown\">\r\n      <li v-for=\"product in filteredProducts\" :key=\"product.id\" class=\"product-card\">\r\n        <div class=\"card-content\">\r\n          <span class=\"product-name\">{{ product.productName }}</span>\r\n          <span class=\"product-price\">S/.{{ product.productPrice }}</span>\r\n        </div>\r\n        <button @click=\"addProductToCart(product)\" class=\"add-button\">+</button>\r\n      </li>\r\n    </ul>\r\n    <button class=\"edit-button\" @click=\"toggleEditMode\">{{ isEditMode ? 'Save' : 