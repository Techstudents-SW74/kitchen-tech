{"ast":null,"code":"import { tablesService } from \"@/public/services/tablesService\";\nexport default {\n  props: {\n    isVisible: {\n      type: Boolean,\n      default: false\n    },\n    restaurantId: Number\n  },\n  data() {\n    return {\n      accountName: \"\",\n      tableNumber: \"\",\n      manualAccountName: \"\",\n      tableId: null\n    };\n  },\n  watch: {\n    tableNumber() {\n      this.fetchTableId();\n    }\n  },\n  methods: {\n    async fetchTableId() {\n      const tables = await tablesService.getTablesByRestaurant(this.restaurantId);\n      const table = tables.find(t => t.tableNumber === this.tableNumber);\n      this.tableId = table ? table.id : null;\n      this.accountName = this.manualAccountName || `Mesa: ${this.tableNumber}`;\n    },\n    handleAccountNameInput(event) {\n      this.manualAccountName = event.target.value;\n    },\n    closeModal() {\n      this.accountName = undefined;\n      this.tableNumber = undefined;\n      this.$emit(\"close-modal\");\n    },\n    save() {\n      if (this.tableId) {\n        this.$emit('save-sale', {\n          accountName: this.accountName,\n          tableId: this.tableId\n        });\n      } else {\n        alert(\"Table ID not found.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["tablesService","props","isVisible","type","Boolean","default","restaurantId","Number","data","accountName","tableNumber","manualAccountName","tableId","watch","fetchTableId","methods","tables","getTablesByRestaurant","table","find","t","id","handleAccountNameInput","event","target","value","closeModal","undefined","$emit","save","alert"],"sources":["E:\\kitchen-tech-repo\\src\\admins\\views\\cassing-views\\components\\save-order-component.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal\" v-if=\"isVisible\">\r\n    <div class=\"modal-content\">\r\n      <div class=\"modal-header\">\r\n        <p class=\"title\">Save Order</p>\r\n        <button class=\"close\" @click=\"closeModal()\">x</button>\r\n      </div>\r\n      <form @submit.prevent=\"save\">\r\n        <div class=\"form-inputs\">\r\n          <div class=\"form-group account-name\">\r\n            <label for=\"accountName\">Account Name</label>\r\n            <input\r\n                type=\"text\"\r\n                id=\"accountName\"\r\n                v-model=\"accountName\"\r\n                @input=\"handleAccountNameInput\"\r\n            />\r\n          </div>\r\n          <div class=\"form-group table-number\">\r\n            <label for=\"tableNumber\">Mesa</label>\r\n            <input\r\n                type=\"text\"\r\n                id=\"tableNumber\"\r\n                v-model=\"tableNumber\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <button type=\"submit\" class=\"save-button\" @click=\"$emit('save-sale', { accountName: this.accountName, tableNumber: this.tableNumber })\">Save Order</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {tablesService} from \"@/public/services/tablesService\";\r\n\r\nexport default {\r\n  props: {\r\n    isVisible: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n    restaurantId: Number,\r\n  },\r\n  data() {\r\n    return {\r\n      accountName: \"\",\r\n      tableNumber: \"\",\r\n      manualAccountName:\"\",\r\n      tableId: null,\r\n    };\r\n  },\r\n  watch: {\r\n    tableNumber() {\r\n      this.fetchTableId();\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchTableId() {\r\n      const tables = await tablesService.getTablesByRestaurant(this.restaurantId);\r\n      const table = tables.find(t => t.tableNumber === this.tableNumber);\r\n      this.tableId = table ? table.id : null;\r\n      this.accountName = this.manualAccountName || `Mesa: ${this.tableNumber}`;\r\n    },\r\n    handleAccountNameInput(event) {\r\n      this.manualAccountName = event.target.value;\r\n    },\r\n    closeModal(){\r\n      this.accountName = undefined;\r\n      this.tableNumber = undefined;\r\n      this.$emit(\"close-modal\");\r\n    },\r\n    save() {\r\n      if (this.tableId) {\r\n        this.$emit('save-sale', { accountName: this.accountName, tableId: this.tableId });\r\n      } else {\r\n        alert(\"Table ID not found.\");\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.6);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.modal-content {\r\n  background-color: #F6F5FA;\r\n  color: #31304A;\r\n  padding: 30px;\r\n  border-radius: 5px;\r\n  width: 550px;\r\n  max-width: 100%;\r\n}\r\n.modal-header{\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n.close{\r\n  background-color: transparent;\r\n  border: none;\r\n  color: #31304A;\r\n  font-weight: 800;\r\n  padding: 0;\r\n  margin: 0;\r\n  align-self: flex-start;\r\n}\r\n\r\n.title{\r\n  font-size: 20px;\r\n  font-weight: 800;\r\n  margin: 0 0 15px 0;\r\n}\r\n.form-inputs{\r\n  display: flex;\r\n}\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n.account-name{\r\n  width: 410px;\r\n}\r\n.table-number{\r\n  margin-left: 24px;\r\n  max-width: 100px;\r\n}\r\n\r\nlabel{\r\n  font-size: 0.8rem;\r\n  margin-left: 5px;\r\n}\r\ninput {\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  background-color: #D3D2E5;\r\n}\r\n.save-button {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #31304A;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n.save-button:active{\r\n  background-color: #201E35;\r\n}\r\n</style>"],"mappings":"AAmCA,SAAQA,aAAa,QAAO,iCAAiC;AAE7D,eAAe;EACbC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,OAAO,EAAE;IACX,CAAC;IACDC,YAAY,EAAEC;EAChB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,iBAAiB,EAAC,EAAE;MACpBC,OAAO,EAAE;IACX,CAAC;EACH