{"ast":null,"code":"import HeaderComponent from \"@/admins/components/header-component.vue\";\nimport SidebarComponent from \"@/admins/components/sidebar-component.vue\";\nimport FavoriteProductCardComponent from \"@/admins/components/favorite-product-card-component.vue\";\nexport default {\n  components: {\n    HeaderComponent,\n    SidebarComponent,\n    FavoriteProductCardComponent\n  },\n  data() {\n    return {\n      restaurantName: '',\n      userRole: '',\n      searchQuery: ''\n    };\n  },\n  mounted() {\n    const userData = JSON.parse(localStorage.getItem('userData'));\n    if (userData) {\n      this.restaurantName = userData['business-name'];\n      this.userRole = userData.role;\n    }\n  },\n  methods: {\n    filterProducts() {\n      const query = this.searchQuery.toLowerCase();\n      this.filteredProducts = this.products.filter(product => product.name.toLowerCase().includes(query));\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","SidebarComponent","FavoriteProductCardComponent","components","data","restaurantName","userRole","searchQuery","mounted","userData","JSON","parse","localStorage","getItem","role","methods","filterProducts","query","toLowerCase","filteredProducts","products","filter","product","name","includes"],"sources":["E:\\kitchen-tech-repo\\src\\admins\\views\\casing-page.vue"],"sourcesContent":["<template>\r\n  <div class=\"layout\">\r\n    <sidebar-component :restaurant-name=\"restaurantName\" :role=\"userRole\" class=\"sidebar\" />\r\n    <div class=\"main-content\">\r\n      <header-component :restaurant-name=\"restaurantName\" :role=\"userRole\" class=\"header\" />\r\n      <div class=\"page-container\">\r\n        <main class=\"products-page\">\r\n          <div class=\"products-header\">\r\n            <input\r\n                class=\"search-bar\"\r\n                type=\"text\"\r\n                v-model=\"searchQuery\"\r\n                placeholder=\"Search products...\"\r\n                @input=\"filterProducts\"\r\n            />\r\n            <button class=\"edit-button\" @click=\"toggleEditMode\">Edit</button>\r\n          </div>\r\n          <div class=\"products-grid\">\r\n            <FavoriteProductCardComponent\r\n              v-for=\"(product, index) in filterProducts\"\r\n              :key=\"product.id\"\r\n              :product=\"product\"\r\n              @product-selected=\"addProductToCart\"\r\n            />\r\n          </div>\r\n\r\n          <button @click=\"sellCustomProduct\">Sell Free Product</button>\r\n        </main>\r\n        <div class=\"cart-summary\">\r\n          <button @click=\"addCustomer\">Add Customer</button>\r\n          <div v-for=\"item in cart\" :key=\"item.id\">{{ item.name }} - S/{{ item.price.toFixed(2) }}</div>\r\n          \r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderComponent from \"@/admins/components/header-component.vue\";\r\nimport SidebarComponent from \"@/admins/components/sidebar-component.vue\";\r\nimport FavoriteProductCardComponent from \"@/admins/components/favorite-product-card-component.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    HeaderComponent,\r\n    SidebarComponent,\r\n    FavoriteProductCardComponent\r\n  },\r\n  data() {\r\n    return {\r\n      restaurantName: '',\r\n      userRole: '',\r\n      searchQuery: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    const userData = JSON.parse(localStorage.getItem('userData'));\r\n    if (userData) {\r\n      this.restaurantName = userData['business-name'];\r\n      this.userRole = userData.role;\r\n    }\r\n  },\r\n  methods: {\r\n    filterProducts() {\r\n      const query = this.searchQuery.toLowerCase();\r\n      this.filteredProducts = this.products.filter((product) => product.name.toLowerCase().includes(query));\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.layout {\r\n  display: flex;\r\n  height: 100vh;\r\n}\r\n\r\n.sidebar {\r\n  width: 280px;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  background-color: #31304A;\r\n}\r\n\r\n.main-content {\r\n  margin-left: 280px; /* Desplaza el contenido a la derecha del sidebar */\r\n  width: calc(100% - 280px); /* Ajusta el ancho para ocupar el resto de la pantalla */\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.header {\r\n  position: fixed;\r\n  top: 0;\r\n  background-color: #5E5E99;\r\n  z-index: 1;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.page-container {\r\n  margin-top: 100px; /* Desplaza el contenido principal por debajo del header */\r\n  padding: 20px;\r\n  background-color: #F6F5FA; /* Fondo blanco para la zona de contenido */\r\n  height: calc(100vh - 100px); /* Ajusta el alto para evitar desbordamientos */\r\n  overflow-y: auto; /* Permite el scroll si el contenido es muy largo */\r\n}\r\n</style>\r\n"],"mappings":"AAuCA,OAAOA,eAAc,MAAO,0CAA0C;AACtE,OAAOC,gBAAe,MAAO,2CAA2C;AACxE,OAAOC,4BAA2B,MAAO,yDAAyD;AAElG,eAAe;EACbC,UAAU,EAAE;IACVH,eAAe;IACfC,gBAAgB;IAChBC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAClBC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,QAAO,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,CAAC;IAC7D,IAAIJ,QAAQ,EAAE;MACZ,IAAI,CAACJ,cAAa,GAAII,QAAQ,CAAC,eAAe,CAAC;MAC/C,IAAI,CAACH,QAAO,GAAIG,QAAQ,CAACK,IAAI;IAC/B;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,IAAI,CAACV,WAAW,CAACW,WAAW,CAAC,CAAC;MAC5C,IAAI,CAACC,gBAAe,GAAI,IAAI,CAACC,QAAQ,CAACC,MAAM,CAAEC,OAAO,IAAKA,OAAO,CAACC,IAAI,CAACL,WAAW,CAAC,CAAC,CAACM,QAAQ,CAACP,KAAK,CAAC,CAAC;IACvG;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}